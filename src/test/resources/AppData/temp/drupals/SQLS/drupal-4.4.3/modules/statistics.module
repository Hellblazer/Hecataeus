SELECT s.nid, s.daycount, s.totalcount, s.timestamp, n.title FROM node_counter s INNER JOIN node n ON s.nid = n.nid;
SELECT a.nid, a.url, a.hostname, a.uid, a.timestamp, n.title FROM accesslog a LEFT JOIN node n ON a.nid = n.nid WHERE a.uid = 0;
SELECT a.nid, a.url, a.hostname, a.uid, MAX(a.timestamp) AS timestamp, n.title FROM accesslog a LEFT JOIN node n ON a.nid = n.nid WHERE a.uid <> '0' GROUP BY a.uid, a.nid, a.url, a.hostname;
SELECT a.nid, a.url, a.hostname, a.uid, a.timestamp, n.title FROM accesslog a INNER JOIN node n ON a.nid = n.nid WHERE a.nid = 0;
SELECT a.nid, a.url, a.hostname, a.uid, a.timestamp, n.title FROM accesslog a LEFT JOIN node n ON a.nid = n.nid WHERE a.hostname = '". check_query($id) ."';
SELECT a.nid, a.url, a.hostname, a.uid, a.timestamp, n.title FROM accesslog a LEFT JOIN node n ON a.nid = n.nid;
SELECT url, MAX(timestamp) AS last_view, COUNT(url) AS cnt FROM accesslog WHERE url <> '' GROUP BY url;
SELECT COUNT(url) FROM accesslog WHERE url <> '';
SELECT url, MAX(timestamp) AS last_view, COUNT(url) AS cnt FROM accesslog WHERE url = '%". check_query($_SERVER["HTTP_HOST"]) ."%' GROUP BY url;
SELECT COUNT(url) FROM accesslog WHERE url <> '' AND url = '%". check_query($_SERVER["HTTP_HOST"]) ."%';
SELECT url, MAX(timestamp) AS last_view, COUNT(url) AS cnt FROM accesslog WHERE url <> '%". check_query($_SERVER["HTTP_HOST"]) ."%' AND url <> '' GROUP BY url;
SELECT COUNT(url) FROM accesslog WHERE url <> '' AND url <> '%". check_query($_SERVER["HTTP_HOST"]) ."%';
SELECT s.nid, ov.title, ov.uid, ov.name FROM node_counter s INNER JOIN ourView ov ON s.nid = ov.nid WHERE ov.status = 1; -- %s <> '0' AND ORDER BY %s DESC;
SELECT totalcount, daycount, timestamp FROM node_counter WHERE nid = 0;
SELECT n.nid, n.title FROM node_counter s INNER JOIN node n ON s.nid = n.nid; -- ORDER BY %s DESC;
