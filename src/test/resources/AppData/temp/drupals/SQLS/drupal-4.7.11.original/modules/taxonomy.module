SELECT tid FROM term_data WHERE vid = 0;
SELECT v.vid, v.*, n.type FROM vocabulary v LEFT JOIN vocabulary_node_types n ON v.vid = n.vid WHERE n.type = '%s' ORDER BY v.weight, v.name;
SELECT v.*, n.type FROM vocabulary v LEFT JOIN vocabulary_node_types n ON v.vid = n.vid ORDER BY v.weight, v.name;
SELECT v.* FROM vocabulary v INNER JOIN vocabulary_node_types n ON v.vid = n.vid WHERE n.type = '%s' ORDER BY v.weight, v.name;
SELECT t.tid, t.* FROM term_data t INNER JOIN term_node r ON r.tid = t.tid WHERE t.vid = 0 AND r.nid = 0 ORDER BY weight;
SELECT t.* FROM term_node r INNER JOIN term_data t ON r.tid = t.tid INNER JOIN vocabulary v ON t.vid = v.vid WHERE r.nid = 0 ORDER BY v.weight, t.weight, t.name;
SELECT t.*, tid1, tid2 FROM term_relation, term_data t WHERE (t.tid = tid1 OR t.tid = tid2) AND (tid1 = 0 OR tid2 = 0) AND t.tid != 0 ORDER BY weight, name;
SELECT t.tid, t.* FROM term_data t INNER JOIN term_hierarchy h ON h.parent = t.tid WHERE h.tid = 0 ORDER BY weight, name;
SELECT t.* FROM term_data t INNER JOIN term_hierarchy h ON h.tid = t.tid WHERE t.vid = 0 AND h.parent = 0 ORDER BY weight, name;
SELECT t.* FROM term_data t INNER JOIN term_hierarchy h ON h.tid = t.tid WHERE parent = 0 ORDER BY weight, name;
SELECT t.tid, t.*, parent FROM term_data t INNER JOIN  term_hierarchy h ON t.tid = h.tid WHERE t.vid = 0 ORDER BY weight, name;
SELECT name FROM term_synonym WHERE tid = 0;
SELECT * FROM term_synonym s, term_data t WHERE t.tid = s.tid AND s.name = '%s';
SELECT t.tid, COUNT(n.nid) AS c FROM term_node t INNER JOIN node n ON t.nid = n.nid WHERE n.status = 1 GROUP BY t.tid;
SELECT t.tid, COUNT(n.nid) AS c FROM term_node t INNER JOIN node n ON t.nid = n.nid WHERE n.status = 1 AND n.type = '%s' GROUP BY t.tid;
SELECT tid, parent FROM term_hierarchy;
SELECT t.tid, t.* FROM term_data t WHERE LOWER('%s') = LOWER(t.name);
SELECT v.*, n.type FROM vocabulary v LEFT JOIN vocabulary_node_types n ON v.vid = n.vid WHERE v.vid = 0 ORDER BY v.weight, v.name;
SELECT * FROM term_data WHERE tid = 0;
SELECT n.nid, n.sticky, n.title, n.created FROM node n INNER JOIN term_node tn ON n.nid = tn.nid WHERE tn.tid IN (0,1) AND n.status = 1 AND n.moderate = 0; -- ORDER BY $order;
SELECT COUNT(n.nid) FROM node n INNER JOIN term_node tn ON n.nid = tn.nid WHERE tn.tid IN (0,1) AND n.status = 1 AND n.moderate = 0;
-- SELECT n.nid, n.sticky, n.title, n.created FROM node n '. $joins .' WHERE n.status = 1 AND n.moderate = 0 '. $wheres .' ORDER BY $order;
-- SELECT COUNT(n.nid) FROM node n '. $joins .' WHERE n.status = 1 AND n.moderate = 0 ' . $wheres;
SELECT t.tid, t.name FROM term_data t WHERE t.tid IN (0,1);
SELECT t.tid, t.name FROM term_data t WHERE t.vid = 0 AND LOWER(t.name) = LOWER('%%%s%%');
